{
  "name": "@swan-form/form",
  "version": "0.1.0",
  "description": "Elegant form handling for React -- contains forms",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/helloeave/swan-form.git"
  },
  "scripts": {
    "build": "NODE_ENV='production' babel --src-type module src --out-dir dist --source-maps --compact=true --no-comments --ignore 'src/__tests__/*,*.test.js'",
    "precommit": "lint-staged",
    "predeploy": "cd example && yarn && yarn run build",
    "deploy": "gh-pages -d example/build",
    "format": "prettier --write '**/*.{json,js,css,md}'",
    "lint": "eslint --config ../../.eslintrc.js src",
    "prepublish": "yarn run build",
    "start": "rollup -c -w",
    "test": "jest --env=jsdom",
    "test:watch": "yarn run test -- --watch"
  },
  "dependencies": {
    "@swan-form/helpers": "^0.1.0",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/preset-react": "^7.0.0-beta.42",
    "@babel/preset-stage-0": "^7.0.0-beta.42",
    "@babel/preset-stage-1": "^7.0.0-beta.42",
    "autoprefixer": "^8.1.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-loader": "8.0.0-beta.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "jest-enzyme": "^6.0.0",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "regenerator-runtime": "^0.11.1",
    "uglify-es": "^3.3.9"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "sideEffects": false
}
