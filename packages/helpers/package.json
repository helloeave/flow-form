{
  "name": "@flow-form/helpers",
  "version": "0.1.0",
  "description": "helpers for flow-form",
  "main": "index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/helloeave/flow-form.git"
  },
  "license": "MIT",
  "scripts": {
    "build":
      "NODE_ENV='production' babel --src-type module src --out-dir dist --ignore src/*.test.js",
    "deploy": "gh-pages -d example/build",
    "format": "prettier --write '**/*.{json,js,css,md}'",
    "precommit": "lint-staged",
    "predeploy": "cd example && npm install && npm run build",
    "prepublish": "npm run build",
    "start": "rollup -c -w",
    "lint": "eslint src",
    "test": "jest --env=jsdom",
    "test:watch": "yarn run test -- --watch"
  }
}
