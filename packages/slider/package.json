{
  "name": "@swan-form/slider",
  "version": "0.3.3",
  "description": "Decompose forms into sliders",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/helloeave/swan-form.git"
  },
  "scripts": {
    "build": "NODE_ENV='production' babel --src-type module src --out-dir dist/cjs --source-maps --compact=true --no-comments --ignore 'src/__tests__/*,*.test.js' && cp src/*.css dist/cjs",
    "precommit": "lint-staged",
    "predeploy": "cd example && yarn && yarn run build",
    "deploy": "gh-pages -d example/build",
    "format": "prettier --write '**/*.{json,js,css,md}'",
    "lint": "eslint src",
    "prepublish": "yarn run build",
    "start": "yarn run build --watch",
    "test": "jest --env=jsdom",
    "test:watch": "yarn run test -- --watch",
    "type": "tsc -p ./tsconfig.json"
  },
  "dependencies": {
    "@swan-form/form": "^0.3.3",
    "@swan-form/helpers": "^0.3.3",
    "invariant": "^2.2.4",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
