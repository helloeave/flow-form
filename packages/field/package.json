{
  "name": "@swan-form/field",
  "version": "0.2.4",
  "description": "Elegant form handling for React -- contains fields",
  "license": "Apache-2.0",
  "author": "Shawn Patrick Rice",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/helloeave/swan-form.git"
  },
  "scripts": {
    "build": "NODE_ENV='production' babel --src-type module src --out-dir dist/cjs --source-maps --compact=true --no-comments --ignore 'src/__tests__/*,*.test.js'",
    "precommit": "lint-staged",
    "format": "prettier --write '**/*.{json,js,css,md}'",
    "lint": "eslint --config ../../.eslintrc.js src",
    "prepublish": "yarn run build",
    "start": "yarn run build --watch",
    "test": "jest --env=jsdom",
    "test:watch": "yarn run test --watch",
    "type": "tsc -p ./tsconfig.json"
  },
  "dependencies": {
    "@swan-form/helpers": "^0.2.4",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
